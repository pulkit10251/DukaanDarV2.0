      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.pulkit10251.DukaanDar",
      "adaptiveIcon": {
        "foregroundImage" : "./assets/adaptive-icon.png",
        "backgroundColor" : "#ffffff"
      },
      "googleServicesFile": "./firebase.js",
      "permissions":["CAMERA", "ACCESS_FINE_LOCATION", "WRITE_EXTERNAL_STORAGE"],
      "versionCode": 1
    }
  }
}
