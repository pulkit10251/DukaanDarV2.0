{
  "expo": {
    "name": "DukaanDar",
    "slug": "DukaanDar",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#003f5c"
    },
    "updates": {
